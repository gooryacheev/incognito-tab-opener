{
  "manifest_version": 3,
  "name": "Open Incognito Tab",
  "version": "1.0",
  "description": "Open a new tab in incognito mode with one click.",
  "permissions": ["tabs", "windows"],
  "action": {
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
